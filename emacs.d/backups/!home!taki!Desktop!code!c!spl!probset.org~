#+begin_src C
#include <stdio.h>

int main(void) {
    float a, b;
    int op;
    scanf("%f %f %d", &a, &b, &op);

    switch (op) {
    case 1:
        printf("Addition: %.2f\n", a+b);
        break;
    case 2:
        printf("Substraction: %.2f\n", a-b);
        break;
    case 3:
        printf("Multiplication: %.2f\n", a*b);
        break;
    case 4:
        printf("Quotient: %d\n", (int)a/b);
        break;
    }
}
#+end_src
** 13
#+begin_src C
#include <stdio.h>

int main(void) {
    float a, b;
    int op;
    scanf("%f %f %d", &a, &b, &op);

    switch (op) {
    case 1:
        printf("Addition: %.2f\n", a+b);
        break;
    case 2:
        printf("Substraction: %.2f\n", a-b);
        break;
    case 3:
        printf("Multiplication: %.2f\n", a*b);
        break;
    case 4:
        scanf("%d", &op);
        switch (op) {
        case 1:
            printf("Quotient: %d\n", (int)(a/b));
            break;
        case 2:
            printf("Remainder: %d\n", (int)a % (int)b);
            break;
        }
        break;
    }
}
#+end_src
** 14
#+begin_src C
#include <stdio.h>

int main(void) {
    float a, b;
    int op;
    scanf("%f %f %d", &a, &b, &op);

    switch (op) {
    case 1:
        printf("Addition: %.2f\n", a+b);
        break;
    case 2:
        printf("Substraction: %.2f\n", a-b);
        break;
    case 3:
        printf("Multiplication: %.2f\n", a*b);
        break;
    case 4:
        if (!b) {
            printf("Error: Divisor is zero\n");
            return 0;
        }

        scanf("%d", &op);
        switch (op) {
        case 1:
            printf("Quotient: %d\n", (int)(a/b));
            break;
        case 2:
            printf("Remainder: %d\n", (int)a % (int)b);
            break;
        }
        break;
    }
}
#+end_src
** 15
#+begin_src C
#include <stdio.h>

int main(void) {
    int correct, guess;
    scanf("%d", &correct);

    for (int i = 1; i <= 3; i++) {
        scanf("%d", &guess);
        if (guess == correct) {
            printf("Right, Player-2 wins!\n");
            return 0;
        }
        printf("Wrong, %d chance(s) left!\n", 3-i);
    }

    printf("Player-1 wins!\n");
}
#+end_src
