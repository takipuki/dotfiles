** 04
#+begin_src C
#include <stdio.h>

int main(void) {
    int x;
    scanf("%d", &x);

    int tmp = x;
    printf("%d\n", tmp++);
    tmp = x;
    printf("%d\n", ++tmp);
    tmp = x;
    printf("%d\n", tmp--);
    tmp = x;
    printf("%d\n", --tmp);
}
#+end_src 
** 05
#+begin_src C
#include <stdio.h>

int main(void) {
    int x, y;
    scanf("%d%d", &x, &y);

    int tmp = x;
    printf("Incremented value: %d\n", tmp += y);
    tmp = x;
    printf("Decremented value: %d\n", tmp -= y);
}
#+end_src 
