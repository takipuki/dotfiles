** 06
#+begin_src C
#include <stdio.h>

int main(void) {
    int n;
    scanf("%d", &n);

    if (!n) {
        printf("Zero not valid!\n");
        return 0;
    } else if (n < 0) {
        printf("Negative not valid!\n");
        return 0;
    }

    if (pow(2, (int)log2(n)) == n)
        printf("Yes\n");
    else
        printf("No\n");
}
#+end_src
