#+begin_src C
#include <stdio.h>

int main(void) {
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);

    printf("a) %d\n", (a+b) <= 80);
    printf("b) %d\n", !(a+b));
    printf("c) %d\n", c != 0);
}
#+end_src 
** 11
#+begin_src C
#include <stdio.h>

int main(void) {
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);

    printf("1) %d\n", (a+b) <= 80 && c >= 0);
    printf("2) %d\n", (a-b) == 0 || c != 0);
    printf("3) %d\n", a != b || !(b < c) && c > 0);
    printf("4) %d\n", (a != b || !(b < c)) && c > 0);
}
#+end_src 
** 12
#+begin_src C
#include <stdio.h>
#include <math.h>

int main(void) {
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);

    int determinant = b*b - 4*a*c;
    if (determinant < 0) {
        printf("Imaginary\n");
        return 0;
    }

    float x1 = (-b + sqrt(determinant)) / (2*a);
    float x2 = (-b - sqrt(determinant)) / (2*a);
    printf("%.2f %.2f\n", x1, x2);
}
#+end_src 
