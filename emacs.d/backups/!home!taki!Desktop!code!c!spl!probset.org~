#+begin_src C
#include <stdio.h>
#include <math.h>

int main(void) {
    float x;
    scanf("%f", &x);

    float rads = x * 3.14159 / 180;
    printf("%f\n", 2*pow(cos(rads), 2) - sqrt(3)*sin(sin(rads)) + log10(x/2));
}
