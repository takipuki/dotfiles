** 09
#+begin_src C
#include <stdio.h>

int main(void) {
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);
    printf("x = %d\n", a - b / 3 + c * 2 - 1);
    printf("y = %d\n", a - (b / ( 3 + c ) * 2) - 1);
    printf("z = %d\n", a - ((b / 3) + c * 2) - 1);
}
#+end_src 
