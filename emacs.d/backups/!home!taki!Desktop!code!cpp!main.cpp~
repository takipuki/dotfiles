#include <iostream>
#include <limits>
using namespace std;
#define nl '\n'
#define deb(n) cout << #n << " = " << (n) << nl
#define FOR(i, init, n) for (int i = (init); i < (n); i++)
#define ROF(i, init, n) for (int i = (init); i >= (n); i--)
#define REP(n) FOR(_, 0, n)
#define skipline() cin.ignore(numeric_limits<streamsize>::max(), nl)
#define UwU ios_base::sync_with_stdio(0),cin.tie(0)
#define yes "YES"
#define no "NO"
typedef unsigned long long ull ;
typedef long long ll;
//typedef long long int;

void solve() {
    int n; cin >> n;
    ll ar[n];
    ll a, k = numeric_limits<ll>::max();
    FOR(i, 0, n) {
        cin >> a;
        if (a < k) k = a;
        ar[i] = a;
    }
    if (n == 1) {
        cout << ar[0] << nl;
        return;
    }

    ll mn = numeric_limits<ll>::max();
    FOR(i, 0, n) {
        a = ar[i] & k;
        if (a < mn) mn = a;
    }
    cout << mn << nl;
}

/**
 * check n = 1
 * int ranges
 */

int main() { UwU;
    int ts; cin >> ts;
    for (;ts--;) {
        solve();
    }
}
