import java.util.ArrayList;

public class Main {
    static class Set<T> {
        int size;
        ArrayList<T> arr;

        public Set() {
            size = 0;
            arr = new ArrayList<>();
        }

        public void add(T e) {
            if (!arr.contains(e)) {
                arr.add(e);
                size++;
            }
        }

        public boolean contains(T e) {
            return arr.contains(e);
        }
    }

    public static void main(String[] args) {
        Set<Integer> myset = new Set<>();
        System.out.println(myset.contains(1));
        myset.add(1);
        System.out.println(myset.contains(1));
    }
}
